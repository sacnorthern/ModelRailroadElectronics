#include <avr/pgmspace.h>

SSD1306_Mini oled;

void setup(){
  randomSeed(analogRead(A2));  //establish 'randomness'
  oled.init(0x3c);             //i2c Address of display
  oled.startScreen();
  oled.clear();
}

void loop() {  //display store,sale,store,trains,store,sale,store,trolleys...random time
               //between displays

    oled.drawImage(STORE_F,0,0,128,8 );    // (name, ulX, ulY, width, height/8)
    delay(random(3,10)*1000);         //delay randomly 3000 to 9000 ms)
